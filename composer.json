{
    "name": "phooty/phooty",
    "description": "Phooty",
    "type": "project",
    "license": "MIT",
    "config": {
        "bin-dir": "bin"
    },
    "authors": [
        {
            "name": "Simon Eddy",
            "email": "simon@simoneddy.com.au"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Phooty\\Core\\": "lib/core/src",
            "Phooty\\Config\\": "lib/config/src",
            "Phooty\\Console\\": "lib/console/src",
            "Phooty\\Foundation\\": "lib/foundation/src",
            "Phooty\\Orm\\": "lib/orm/src",
            "Phooty\\Support\\": "lib/support/src",
            "Phooty\\Crawler\\": "lib/crawler/src",
            "Phooty\\Events\\": "lib/events/src"

        },
        "files": [
            "lib/support/src/helpers.php"
        ]
    },
    "require": {
        "symfony/console": "^4.2",
        "symfony/process": "^4.2",
        "illuminate/config": "^5.8",
        "illuminate/support": "^5.8",
        "illuminate/container": "^5.8",
        "ramsey/uuid-doctrine": "^1.5",
        "ramsey/uuid": "^3.8",
        "doctrine/orm": "^2.6",
        "nesbot/carbon": "^2.17",
        "symfony/config": "^4.2",
        "symfony/yaml": "^4.2",
        "symfony/dom-crawler": "^4.2",
        "symfony/css-selector": "^4.2",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.2"
    }
}

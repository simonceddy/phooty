{
    "name": "phooty/phooty",
    "description": "Phooty",
    "type": "project",
    "license": "MIT",
    "config": {
        "bin-dir": "bin"
    },
    "authors": [
        {
            "name": "Simon Eddy",
            "email": "simon@simoneddy.com.au"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Phooty\\Console\\": "lib/console/src"
        }
    },
    "require": {
        "php": ">=7.1",
        "symfony/console": "^4.2",
        "symfony/process": "^4.2",
        "illuminate/config": "^5.8",
        "illuminate/support": "^5.8",
        "illuminate/container": "^5.8",
        "ramsey/uuid-doctrine": "^1.5",
        "ramsey/uuid": "^3.8",
        "doctrine/orm": "^2.6",
        "nesbot/carbon": "^2.17",
        "phooty/orm": "dev-master",
        "phooty/config": "dev-master",
        "phooty/foundation": "dev-master",
        "phooty/crawler": "dev-master",
        "symfony/config": "^4.2",
        "symfony/yaml": "^4.2",
        "symfony/dom-crawler": "^4.2",
        "symfony/css-selector": "^4.2",
        "guzzlehttp/guzzle": "^6.3",
        "vlucas/phpdotenv": "^3.3",
        "react/event-loop": "^1.1",
        "react/stream": "^1.1",
        "react/promise": "^2.7",
        "react/child-process": "^0.6.1",
        "react/promise-stream": "^1.1",
        "react/cache": "^0.5.0",
        "react/promise-timer": "^1.5"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.2",
        "phpunit/phpunit": "^8.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:phooty/crawler.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:phooty/foundation.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:phooty/orm.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:phooty/config.git"
        }
    ]
}

{
    "name": "simoneddy/phooty",
    "description": "Aussie Rules football simulator written in PHP",
    "type": "project",
    "license": "MIT",
    "config": {
        "bin-dir": "bin"
    },
    "authors": [
        {
            "name": "Simon Eddy",
            "email": "simon@simoneddy.com.au"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Phooty\\": "src"
        },
        "files": [
            
        ]
    },
    "require": {
        "evenement/evenement": "^3.0",
        "pimple/pimple": "^3.2",
        "symfony/yaml": "^5.0",
        "vlucas/phpdotenv": "^3.6",
        "react/event-loop": "^1.1",
        "react/stream": "^1.1",
        "php-ds/php-ds": "^1.2",
        "doctrine/cache": "^1.10"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.8",
        "phpspec/phpspec": "^6.0",
        "symfony/var-dumper": "^5.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/simonceddy/dot-config-loader"
        }
    ]
}

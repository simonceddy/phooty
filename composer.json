{
    "name": "simoneddy/phooty",
    "description": "Aussie Rules football simulator written in PHP",
    "type": "project",
    "license": "MIT",
    "config": {
        "bin-dir": "bin"
    },
    "authors": [
        {
            "name": "Simon Eddy",
            "email": "simon@simoneddy.com.au"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Phooty\\": "src"
        }
    },
    "require": {
        "evenement/evenement": "^3.0",
        "simoneddy/path": "^0.0.2",
        "adbario/php-dot-notation": "^2.2",
        "fzaninotto/faker": "^1.8",
        "pimple/pimple": "^3.2",
        "symfony/yaml": "^4.3",
        "symfony/filesystem": "^4.3",
        "beberlei/assert": "^3.2",
        "vlucas/phpdotenv": "^3.6"
    },
    "require-dev": {
        "consolidation/robo": "^1.4",
        "behat/behat": "^3.5",
        "phpspec/phpspec": "^5.1",
        "symfony/var-dumper": "^4.3"
    }
}

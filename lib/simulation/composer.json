{
    "name": "phooty/core",
    "description": "Phooty simulation",
    "type": "library",
    "license": "MIT",
    "config": {
        "bin-dir": "bin"
    },
    "authors": [
        {
            "name": "Simon Eddy",
            "email": "simon@simoneddy.com.au"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Phooty\\Simulation\\": "src"
        }
    },
    "require": {
        "php": ">=7.1",
        "symfony/event-dispatcher": "^4.2",
        "illuminate/container": "^5.8",
        "ramsey/uuid": "^3.8",
        "simoneddy/tilemap": "dev-master",
        "phooty/config": "dev-master",
        "symfony/yaml": "^4.2",
        "nesbot/carbon": "^2.18"
    },
    "require-dev": {
        "simoneddy/tilemap-visualizer": "dev-master",
        "symfony/var-dumper": "^4.2",
        "fzaninotto/faker": "^1.8"
    },
    "bin": [
        "bin/phooty-sim"
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:simonceddy/tilemap.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:simonceddy/tilemap-visualizer.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:phooty/config.git"
        }
    ]
}
